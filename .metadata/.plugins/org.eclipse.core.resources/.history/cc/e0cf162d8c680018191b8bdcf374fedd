	@Test
	public void Demo2(){
		 List<Integer> nums = Arrays.asList(1,1,null,2,4,3,4,null,5,6,7,8,9,10); 
		 System.out.println("ÇóºÍ:"+nums.stream().filter(e -> e !=null).distinct().mapToInt(num -> 2*num).peek(System.out::println).sum());
		
	}
	@Test
